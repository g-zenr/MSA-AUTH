	"metaFieldGuidelines": {
		"commonFields": {
			"capacity": "Number - maximum occupancy/usage",
			"operatingHours": "Object/String - when facility is available",
			"accessRequirements": "Array - what's needed for access",
			"equipment": "Array - available equipment/amenities",
			"bookingRules": "Object - reservation and usage rules",
			"specialFeatures": "Array - unique features of this facility"
		},
		"hotelSpecific": {
			"roomNumber": "String - unique room identifier",
			"view": "String - what the room overlooks",
			"bedConfiguration": "String - bed setup",
			"hasBalcony": "Boolean - balcony availability",
			"wifiNetwork": "String - dedicated wifi network"
		},
		"restaurantSpecific": {
			"tableCount": "Number - number of tables",
			"seatingCapacity": "Number - total seats",
			"cuisineType": "String - type of cuisine",
			"isOpenAir": "Boolean - outdoor seating",
			"hasBar": "Boolean - bar availability"
		},
		"parkingSpecific": {
			"totalSlots": "Number - total parking spaces",
			"slotsAvailable": "Number - currently available",
			"vehicleTypes": "Array - supported vehicle types",
			"maxVehicleHeight": "Number - height restriction",
			"evChargingStations": "Number - EV charging points"
		},
		"sportsSpecific": {
			"courtSurface": "String - playing surface material",
			"isIndoor": "Boolean - indoor vs outdoor",
			"maxPlayers": "Number - maximum players",
			"equipmentProvided": "Array - included equipment"
		},
		"conferenceSpecific": {
			"seatingCapacity": "Number - meeting capacity",
			"roomLayout": "String - seating arrangement",
			"audioVisual": "Object - AV equipment details",
			"catering": "Object - food service options"
		}
	},
	"usage": {
		"description": "The meta field provides flexible storage for facility-specific information that doesn't apply to all facility types. This approach avoids rigid schema constraints while maintaining data structure.",
		"benefits": [
			"No unused fields - each facility only stores relevant data",
			"Future-proof - new facility types can add custom fields",
			"Flexible queries - can search within meta JSON",
			"Type-specific features - each category has its own relevant metadata"
		],
		"implementation": [
			"Store as JSON in the meta field",
			"Validate meta structure based on facility type",
